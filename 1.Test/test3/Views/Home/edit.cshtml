
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>edit</title>
</head>
<body>
    <form method="post" action="@Url.Action("add_")">
        <fieldset>
            <legend> <center> <h2> @ViewBag.message</h2> </center></legend>
            <div>

                <label>شماره دانشجویی</label>
                <div>
                    <input type="number" name="StdId" value= @Model.StdID>
                </div>
            </div>




            <div>
                <label>نام و نام خانوادگی</label>
                <div>
                    <input type="text" name="FullName" value= @Model.name />
                </div>
            </div>

            <div>
                <label>رشته</label>
                <div>
                    <input type="text" name="Field" value= @Model.Field" />
                </div>
            </div>


            <div>
                <div>
                    <input type="submit" value="ویرایش اطلاعات دانشجو" />
                </div>
            </div>

        </fieldset>
        </div>
</body>
</html>
